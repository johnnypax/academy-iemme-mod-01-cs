@model List<ASP_WEB_lez01_test.Models.Persona>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Nominativo</th>
            <th>Codice Fiscale</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Persona per in Model)
        {
            <tr>
                <td>@per.Nominativo</td>
                <td>@per.CodFis</td>
            </tr>
        }
    </tbody>
</table>